/*底部导航样式*/
.nav-footer, .checkout, .goods-detual-bottom {
	width: 100%;
	position: fixed;
	height: 3.5rem;
	left: 0;
	bottom: 0;
	z-index: 10;
	/*background-color: #00bd0d;*/
}
.nav-footer {
	height: calc(3.5rem - 1px);
	border-top: 1px solid #d9d9d9;
	background-color: #f2f2f2;
}
.nav-footer ul {
	overflow: hidden;
}
.nav-footer ul li {
	height: calc(3.5rem - 1px);
}
.footer-item {
	padding-top: calc(0.2rem - 1px);
	height: calc(100% - 0.2rem);
	width: 100%;
	position: relative;
}
.footer-item-text {
	font-size: 0.8rem;
	height: 0.9rem;
	line-height: 0.9rem;
	margin-top: 0.2rem;
	color: #787878;
}

.footer-item img {
	height: 1.5rem;
	position: relative;
    top: 0.2rem;
}
.nav-footer ul li.active {
	background-color: #1e1e1e;
}
.cf-icon-home {
	background: url(../images/cf-icon-home.png) no-repeat;
	height: 1.6rem;
	width: 1.6rem;
}
.cf-icon-me {
	background: url(../images/cf-icon-me.png) no-repeat;
	height: 1.6rem;
	width: 1.6rem;
}
.cf-icon-select {
	position: absolute;
	top: 0;
	left: calc(50% - 2rem);
	background: url(../images/cf-icon-select.png) no-repeat;
	height: calc(3.5rem - 1px);
	width: calc(3.5rem - 1px);
	background-size: 100% 100%;
}
.cf-icon-select-click {
	position: absolute;
	top: 0;
	left: calc(50% - 2rem);
	background: url(../images/cf-icon-select-active.png) no-repeat;
	height: 3.5rem;
	width: 3.5rem;
	background-size: 100% 100%;
}
.cf-icon-home,  .cf-icon-me {
    position: relative;
    top: 0.1rem;
    background-size: 100% 100%;
}
.footer-item.active .cf-icon-home {
	background: url(../images/cf-icon-home-active.png) no-repeat;
	background-size: 100% 100%;
}
.footer-item.active .cf-icon-me {
	background: url(../images/cf-icon-me-active.png) no-repeat;
	background-size: 100% 100%;
}
.footer-item.active .footer-item-text {
	color: #f55a60;
}
