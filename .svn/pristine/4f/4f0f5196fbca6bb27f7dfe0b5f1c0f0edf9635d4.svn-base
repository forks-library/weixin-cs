/*通用样式*/
/*背景图片样式*/

.cf-icon-home, .cf-icon-me, .cf-icon-select, .cf-icon-select-click {
	display: inline-block;
}
/*
**单选、多选样式
*/
.regular-box + label , .regular-radio + label {	
	background-color: #fafafa;
	border: 1px solid #cacece;
	box-shadow: 0 1px 2px rgba(0,0,0,0.05) , inset 0px -15px 10px -12px rgba(0,0,0,0.05);
	padding: 6px;
	display: inline-block;
	position: relative;
	top: 2px;
	border-radius: 50%;
}
.regular-box:checked + label:after , .regular-radio:checked + label:after {
	content: ' ';
	width: 8px;
	height: 8px;
	position: absolute;
	top: 2px;
	background: #99a1a7;
	box-shadow: inset 0px 0px 10px rgba(0,0,0,0.3);
	text-shadow: 0px;
	left: 2px;
	border-radius: 50%;
}
.regular-box:checked + label , .regular-radio:checked + label {
	background-color: #e9ecee;
	color: #99a1a7;
	border: 1px solid #adb8c0;
	box-shadow: 0 1px 2px rgba(0,0,0,0.05) , inset 0px -15px 10px -12px rgba(0,0,0,0.05) , inset 15px 10px -12px rgba(255,255,255,0.1) , inset 0px 0px 10px rgba(0,0,0,0.1);
}
.regular-radio + label:active , .regular-radio:checked + label:active ,
.regular-box + label:active , .regular-box:checked + label:active {
	box-shadow: 0 1px 2px rgba(0,0,0,0.05) , inset 0px 1px 3px rgba(0,0,0,0.1);
}
select {
	appearance:none;
	-moz-appearance:none;
	-webkit-appearance:none;
	-ms-appearance:none;
	border: none;
	height: 2rem;
	background-color: #fff;
}

