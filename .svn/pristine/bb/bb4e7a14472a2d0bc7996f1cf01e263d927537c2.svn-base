{{if datas}}
{{each datas as news i}}
<li class="aclick-btn" data-url="et.redirect(et.url('info/aboutus/newspage', {no: '{{news.code}}'}));">
	<div class="news-list-img">
		<img class="width100 height100" src="{{news.first}}" alt="" />
	</div>
	<div class="news-list-cont">
		<div class="news-cont-title ellipsis ">{{news.title}}</div>
		<div class="news-cont-intro">{{news.note}}</div>
		<div class="news-cont-date">{{news.time}}</div>
		<div class="news-cont-btn color-white">详情</div>
	</div>
</li>
{{/each}}
{{/if}}